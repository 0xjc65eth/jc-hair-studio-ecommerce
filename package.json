    "seo:submit:all": "bash submit-all-search-engines.sh",
    "seo:generate-feed": "node scripts/generate-product-feed.mjs",
    "seo:generate-csv": "node scripts/generate-merchant-csv.mjs",
    "seo:generate-rss": "npx tsx scripts/generate-rss-feed.ts",
    "seo:validate-rss": "npx tsx scripts/validate-rss-feed.ts",
    "seo:check-rss": "npx tsx scripts/check-rss-status.ts",
