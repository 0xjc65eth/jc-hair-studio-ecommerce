    "test:newsletter": "playwright test tests/e2e/newsletter.spec.ts",
    "test:account": "playwright test tests/e2e/account.spec.ts",
    "test:payments": "playwright test tests/e2e/payment-flows.spec.ts",
    "test:comprehensive-cart": "playwright test tests/e2e/comprehensive-cart.spec.ts",
