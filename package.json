  "name": "jc-hair-studio",
  "version": "1.0.0",
  "description": "E-commerce de extensÃµes de cabelo - JC Hair Studio's 62",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "prebuild": "node scripts/generate-product-feed.mjs",
    "build": "next build",
    "build:production": "NODE_ENV=production npm run build:pre-check && npm run build:optimized",
    "build:vercel": "echo 'Starting Vercel optimized build...' && NODE_OPTIONS='--max-old-space-size=8192' SKIP_TYPE_CHECK=true SKIP_LINT=true NEXT_TELEMETRY_DISABLED=1 next build",
    "build:optimized": "npm run optimize:build && npm run build:vercel",
    "build:check": "npm run type-check:safe && npm run lint:safe",
    "build:pre-check": "echo 'Running pre-build checks...' && npm run type-check:production && npm run lint:production",
    "start": "next start",
    "start:production": "NODE_ENV=production next start -p $PORT",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix --quiet",
    "lint:safe": "eslint . --quiet || echo 'Lint warnings ignored'",
    "lint:production": "SKIP_LINT=true eslint . --quiet --max-warnings=100 || echo 'Lint check completed with warnings'",
    "type-check": "tsc --noEmit",
    "type-check:safe": "tsc --noEmit --skipLibCheck || echo 'Type check warnings ignored'",
    "type-check:production": "SKIP_TYPE_CHECK=true tsc --noEmit --skipLibCheck --incremental false || echo 'Type check completed with warnings'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:cart": "playwright test tests/e2e/cart-checkout.spec.ts",
