{
  "name": "jc-hair-studio",
  "version": "1.0.0",
  "description": "E-commerce de extens√µes de cabelo - JC Hair Studio's 62",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "build:safe": "SKIP_TYPE_CHECK=true SKIP_LINT=true next build",
    "build:vercel": "npx prisma generate && SKIP_TYPE_CHECK=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start -p $PORT",
    "lint": "next lint --quiet",
    "lint:fix": "next lint --fix --quiet",
    "lint:safe": "next lint --quiet || echo 'Lint warnings ignored'",
    "type-check": "tsc --noEmit",
    "type-check:safe": "tsc --noEmit --skipLibCheck || echo 'Type check warnings ignored'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:cart": "playwright test tests/e2e/cart-checkout.spec.ts",
    "test:checkout": "playwright test tests/e2e/cart-checkout.spec.ts",
    "test:newsletter": "playwright test tests/e2e/newsletter.spec.ts",
    "test:account": "playwright test tests/e2e/account.spec.ts",
    "test:payments": "playwright test tests/e2e/payment-flows.spec.ts",
    "test:comprehensive-cart": "playwright test tests/e2e/comprehensive-cart.spec.ts",
    "test:comprehensive-checkout": "playwright test tests/e2e/comprehensive-checkout.spec.ts",
    "test:email": "node scripts/test-email.js",
    "test:local": "npm run lint && npm run type-check && npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:pipeline": "npm run test:local && npm run test:email",
    "test:comprehensive": "node scripts/comprehensive-test-reporter.js",
    "test:loop": "node scripts/test-loop.js",
    "test:agents": "node scripts/multi-agent-tests.js",
    "test:accessibility": "node tests/accessibility-minimal.mjs",
    "test:accessibility:headless": "TEST_HEADLESS=true node tests/accessibility-minimal.mjs",
    "test:performance": "node tests/performance-test.mjs",
    "test:performance:headless": "TEST_HEADLESS=true node tests/performance-test.mjs",
    "test:all": "npm run test:pipeline && npm run test:accessibility:headless && npm run test:performance:headless",
    "prepare": "node -e \"try{require('husky')}catch(e){process.exit(0)}\" && husky || true",
    "seed:mongodb": "tsx scripts/seed-mongodb.ts",
    "analyze": "ANALYZE=true next build",
    "postinstall": "npx prisma generate || echo 'Prisma generation failed, continuing...'",
    "prisma:generate": "npx prisma generate",
    "prisma:deploy": "npx prisma db push",
    "vercel:build": "npx prisma generate && SKIP_TYPE_CHECK=true next build",
    "cleanup:build": "rm -rf .next dist node_modules/.cache"
  },
  "keywords": [
    "ecommerce",
    "hair",
    "extensions",
    "nextjs"
  ],
  "author": "JC Hair Studio",
  "license": "MIT",
  "dependencies": {
