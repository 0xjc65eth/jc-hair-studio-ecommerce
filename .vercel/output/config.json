{
  "version": 3,
  "routes": [
    {
      "src": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308,
      "continue": true
    },
    {
      "src": "/_next/__private/trace",
      "dest": "/404",
      "status": 404,
      "continue": true
    },
    {
      "src": "^(?:/(.*))(?:/)?$",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "SAMEORIGIN",
        "X-XSS-Protection": "1; mode=block"
      },
      "continue": true
    },
    {
      "src": "/404/?",
      "status": 404,
      "continue": true,
      "missing": [
        {
          "type": "header",
          "key": "x-prerender-revalidate"
        }
      ]
    },
    {
      "src": "/500",
      "status": 500,
      "continue": true
    },
    {
      "src": "^/home$",
      "headers": {
        "Location": "/"
      },
      "status": 308
    },
    {
      "src": "^/api(?:/(.*))$",
      "headers": {
        "Cache-Control": "public, max-age=300, s-maxage=300, stale-while-revalidate=59"
      },
      "continue": true
    },
    {
      "src": "^(?:/(.*))$",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "SAMEORIGIN",
        "X-XSS-Protection": "1; mode=block"
      },
      "continue": true
    },
    {
      "src": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?$",
      "headers": {
        "x-authorized": "true"
      },
      "continue": true,
      "has": [
        {
          "type": "query",
          "key": "authorized"
        }
      ]
    },
    {
      "src": "^/?",
      "has": [
        {
          "type": "header",
          "key": "rsc"
        }
      ],
      "dest": "/index.rsc",
      "continue": true,
      "override": true
    },
    {
      "src": "^/((?!.+\\.rsc).+?)(?:/)?$",
      "has": [
        {
          "type": "header",
          "key": "rsc"
        }
      ],
      "dest": "/$1.rsc",
      "continue": true,
      "override": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/index(\\.action|\\.rsc)",
      "dest": "/",
      "continue": true
    },
    {
      "src": "^/sitemap\\.xml$",
      "dest": "/api/sitemap",
      "check": true
    },
    {
      "src": "/_next/data/(.*)",
      "dest": "/_next/data/$1",
      "check": true
    },
    {
      "src": "/\\.prefetch\\.rsc$",
      "dest": "/__index.prefetch.rsc",
      "check": true
    },
    {
      "src": "/(.+)/\\.prefetch\\.rsc$",
      "dest": "/$1.prefetch.rsc",
      "check": true
    },
    {
      "src": "/\\.rsc$",
      "dest": "/index.rsc",
      "check": true
    },
    {
      "src": "/(.+)/\\.rsc$",
      "dest": "/$1.rsc",
      "check": true
    },
    {
      "handle": "resource"
    },
    {
      "src": "/.*",
      "status": 404
    },
    {
      "handle": "miss"
    },
    {
      "src": "/_next/static/(?:[^/]+/pages|pages|chunks|runtime|css|image|media)/.+",
      "status": 404,
      "check": true,
      "dest": "$0"
    },
    {
      "handle": "rewrite"
    },
    {
      "src": "/_next/data/(.*)",
      "dest": "/404",
      "status": 404
    },
    {
      "src": "^/api/auth/(?<nxtPnextauth>.+?)(?:\\.rsc)(?:/)?$",
      "dest": "/api/auth/[...nextauth].rsc?nxtPnextauth=$nxtPnextauth"
    },
    {
      "src": "^/api/auth/(?<nxtPnextauth>.+?)(?:/)?$",
      "dest": "/api/auth/[...nextauth]?nxtPnextauth=$nxtPnextauth"
    },
    {
      "src": "^/api/cart/(?<nxtPitemId>[^/]+?)(?:\\.rsc)(?:/)?$",
      "dest": "/api/cart/[itemId].rsc?nxtPitemId=$nxtPitemId"
    },
    {
      "src": "^/api/cart/(?<nxtPitemId>[^/]+?)(?:/)?$",
      "dest": "/api/cart/[itemId]?nxtPitemId=$nxtPitemId"
    },
    {
      "src": "^/api/products/brands/(?<nxtPbrand>[^/]+?)(?:\\.rsc)(?:/)?$",
      "dest": "/api/products/brands/[brand].rsc?nxtPbrand=$nxtPbrand"
    },
    {
      "src": "^/api/products/brands/(?<nxtPbrand>[^/]+?)(?:/)?$",
      "dest": "/api/products/brands/[brand]?nxtPbrand=$nxtPbrand"
    },
    {
      "src": "^/categoria/(?<nxtPslug>[^/]+?)(?:\\.rsc)(?:/)?$",
      "dest": "/categoria/[slug].rsc?nxtPslug=$nxtPslug"
    },
    {
      "src": "^/categoria/(?<nxtPslug>[^/]+?)(?:/)?$",
      "dest": "/categoria/[slug]?nxtPslug=$nxtPslug"
    },
    {
      "src": "^/produto/(?<nxtPid>[^/]+?)(?:\\.rsc)(?:/)?$",
      "dest": "/produto/[id].rsc?nxtPid=$nxtPid"
    },
    {
      "src": "^/produto/(?<nxtPid>[^/]+?)(?:/)?$",
      "dest": "/produto/[id]?nxtPid=$nxtPid"
    },
    {
      "handle": "hit"
    },
    {
      "src": "/_next/static/(?:[^/]+/pages|pages|chunks|runtime|css|image|media|a7ty8JsrIbioDhJ_U02NY)/.+",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true,
      "important": true
    },
    {
      "src": "/index(?:/)?",
      "headers": {
        "x-matched-path": "/"
      },
      "continue": true,
      "important": true
    },
    {
      "src": "/((?!index$).*?)(?:/)?",
      "headers": {
        "x-matched-path": "/$1"
      },
      "continue": true,
      "important": true
    },
    {
      "handle": "error"
    },
    {
      "src": "/.*",
      "dest": "/404",
      "status": 404
    },
    {
      "src": "/.*",
      "dest": "/500",
      "status": 500
    }
  ],
  "overrides": {
    "404.html": {
      "path": "404",
      "contentType": "text/html; charset=utf-8"
    },
    "500.html": {
      "path": "500",
      "contentType": "text/html; charset=utf-8"
    },
    "_app.rsc.json": {
      "path": "_app.rsc",
      "contentType": "application/json"
    },
    "_error.rsc.json": {
      "path": "_error.rsc",
      "contentType": "application/json"
    },
    "_document.rsc.json": {
      "path": "_document.rsc",
      "contentType": "application/json"
    },
    "404.rsc.json": {
      "path": "404.rsc",
      "contentType": "application/json"
    }
  },
  "framework": {
    "version": "15.5.3"
  },
  "crons": []
}
